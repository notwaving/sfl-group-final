<h1>Listing phone_rentals</h1>

<table>
  <thead>
    <tr>
      <th>Phone</th>
      <th>User</th>
      <th>Data</th>
      <th>Charger</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_rentals.each do |phone_rental| %>
      <tr>
        <td><%= phone_rental.phone_id %></td>
        <td><%= phone_rental.user_id %></td>
        <td><%= phone_rental.data %></td>
        <td><%= phone_rental.charger %></td>
        <td><%= link_to 'Show', phone_rental %></td>
        <td><%= link_to 'Edit', edit_phone_rental_path(phone_rental) %></td>
        <td><%= link_to 'Destroy', phone_rental, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone rental', new_phone_rental_path %>
